<body>
    <div class="wt-container">
        <header> <!-- style="display: none;"-->
            <div class="wt-content-wrap">
                <!--noindex-->
                <a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}" class="wt-logo" title="Main page"></a> <!--rel="nofollow"-->
                <!--noindex-->
                <div class="wt-top-contacts">
                    <div class="wt-lenguage"><span class="wt-flag" style="background-image: url('/libs/img/flag-{{ session.language | upper }}.webp')"></span><p>{{ session.language | upper }}</p><span class="wt-angle-down"></span>
                        <ul class="wt-dropdown">
                            {% for key in locales %}
                            {% set ar_lang = 'ar' %}
                                {% if key.slug != session.language %}
                                    {% if key.slug == baselocale %}
                                        <li><a class="wt-lang-link" href="{{ home_url }}{{ path }}{#
                                            check for proguct cart #}{% if not (product_slug is empty) %}{{ '/' ~ p_slug[baselocale] }}{% endif %}{#
                                            check for catalog filters #}{% if c_slug|length > 0 %}{{ '/' ~ c_slug[baselocale] }}{% endif %}" title="{{ key.name }}">{#
                                            #}<span class="wt-flag" style="background-image: url('/libs/img/flag-{{ key.slug | upper }}.webp')"></span>{{ key.slug | upper }}</a></li>
                                    {% elseif key.slug == ar_lang %}
                                        <li><a class="wt-lang-link" href="http://woodtrade-arabic.test/{{ path }}{#
                                            check for proguct cart #}{% if not (product_slug is empty) %}{{ '/' ~ p_slug[key.slug] }}{% endif %}{#
                                            check for catalog filters #}{% if c_slug|length > 0 %}{{ '/' ~ c_slug[key.slug] }}{% endif %}" title="{{ key.name }}">{#
                                            #}<span class="wt-flag" style="background-image: url('/libs/img/flag-{{ key.slug | upper }}.webp')"></span>{{ key.slug | upper }}</a></li>
                                    {% else %}
                                        <li><a class="wt-lang-link" href="{{ home_url }}{{ key.slug  }}/{{ path }}{#
                                            check for proguct cart #}{% if not (product_slug is empty) %}{{ '/' ~ p_slug[key.slug] }}{% endif %}{#
                                            check for catalog filters #}{% if c_slug|length > 0 %}{{ '/' ~ c_slug[key.slug] }}{% endif %}" title="{{ key.name }}">{#
                                            #}<span class="wt-flag" style="background-image: url('/libs/img/flag-{{ key.slug | upper }}.webp')"></span>{{ key.slug | upper }}</a></li>
                                    {% endif %}
                                {% endif %}
                            {% endfor %}
                        </ul>
                    </div>

                    <div class="wt-inner-grid">
                        <p><a href="tel:{{ our_number_link_header }}" title="{{ our_number_header }}">{{ our_number_header }}</a></p>
                        <a class="wt-button-contact" href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}contact" title="{{ contact_us }}">{{ contact_us }}</a>
                    </div>

                </div>
                <nav class="wt-top-menu" id="top_menu" role="navigation">
                    <ul class="wt-menu-ul">
                        <li class="wt-burger"></li>
                        <li class="wt-item" id="products"><a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}catalog" title="">{{ products_menu }}</a><span class="wt-angle-down" onclick="menu('#products')"></span>
                            <div class="wt-dropdown-menu-wrap">
                                <ul class="wt-dropdown-menu">
                                    {% for category in categories %}
                                        <li class="wt-dropdown-item"><a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}catalog/{{ category.slug }}" title="{{ category.name }}">{{ category.name }}</a></li>
                                    {% endfor %}
                                </ul>
                            </div>
                        </li>
                        <li class="wt-item" id="services"><a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}services" title="{{ service_header }}">{{ service_header }}</a><span class="wt-angle-down" onclick="menu('#services')"></span>
                            <div class="wt-dropdown-menu-wrap">
                                <ul class="wt-dropdown-menu">
                                    <li class="wt-dropdown-item"><a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}sawing-wood" title="{{ service_header_1 }}">{{ service_header_1 }}</a></li>
                                    <li class="wt-dropdown-item"><a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}wood-drying" title="{{ service_header_2 }}">{{ service_header_2 }}</a></li>
                                    <li class="wt-dropdown-item"><a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}grinding-and-calibration" title="{{ service_header_3 }}">{{ service_header_3 }}</a></li>
                                </ul>
                            </div>
                        </li>
                        <li class="wt-item" id="about_company"><a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}about" title="{{ about_conpany_header }}">{{ about_conpany_header }}</a><span class="wt-angle-down" onclick="menu('#about_company')"></span>
                            <div class="wt-dropdown-menu-wrap">
                                <ul class="wt-dropdown-menu">
                                    <li class="wt-dropdown-item"><a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}production-capacity" title="{{ about_conpany_header_1 }}">{{ about_conpany_header_1 }}</a></li>
                                    <li class="wt-dropdown-item"><a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}contact" title="{{ about_conpany_header_2 }}">{{ about_conpany_header_2 }}</a></li>
                                </ul>
                            </div>
                        </li>
                        <li class="wt-info"><a href="tel:{{ our_number_link_header }}" title="{{ our_number_header }}">{{ our_number_header }}</a></li>
                        <li class="wt-info"><a href="mailto:{{ our_email_header }}" title="{{ our_email_header }}">{{ our_email_header }}</a></li>
                        <li class="wt-info"><a href="{{ header_show_on_map_link }}" title="{{ our_adress }}">{{ our_adress }}</a></li>
                        <li class="wt-feedback"><a href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}contact" title="{{ contact_us }}">{{ contact_us }}</a></li>
                        <li class="wt-lenguage"><span class="wt-flag" style="background-image: url('/libs/img/flag-{{ session.language | upper }}.webp')"></span><p>{{ session.language | upper }}</p><span class="wt-angle-down"></span>
                            <ul class="wt-dropdown">
                                {% for key in locales %}
                                {% set ar_lang = 'ar' %}
                                    {% if key.slug != session.language %}
                                        {% if key.slug == baselocale %}
                                            <li><a class="wt-lang-link" href="{{ home_url }}{{ path }}{#
                                                check for proguct cart #}{% if not (product_slug is empty) %}{{ '/' ~ p_slug[baselocale] }}{% endif %}{#
                                                check for catalog filters #}{% if c_slug|length > 0 %}{{ '/' ~ c_slug[baselocale] }}{% endif %}" title="{{ key.name }}">{#
                                                #}<span class="wt-flag" style="background-image: url('/libs/img/flag-{{ key.slug | upper }}.webp')"></span>{{ key.slug | upper }}</a></li>
                                        {% elseif key.slug == ar_lang %}
                                            <li><a class="wt-lang-link" href="{{ change_site_url }}{{ path }}{#
                                                check for proguct cart #}{% if not (product_slug is empty) %}{{ '/' ~ p_slug[key.slug] }}{% endif %}{#
                                                check for catalog filters #}{% if c_slug|length > 0 %}{{ '/' ~ c_slug[key.slug] }}{% endif %}" title="{{ key.name }}">{#
                                                #}<span class="wt-flag" style="background-image: url('/libs/img/flag-{{ key.slug | upper }}.webp')"></span>{{ key.slug | upper }}</a></li>
                                        {% else %}
                                            <li><a class="wt-lang-link" href="{{ home_url }}{{ key.slug }}/{{ path }}{#
                                                check for proguct cart #}{% if not (product_slug is empty) %}{{ '/' ~ p_slug[key.slug] }}{% endif %}{#
                                                check for catalog filters #}{% if c_slug|length > 0 %}{{ '/' ~ c_slug[key.slug] }}{% endif %}" title="{{ key.name }}">{#
                                                #}<span class="wt-flag" style="background-image: url('/libs/img/flag-{{ key.slug | upper }}.webp')"></span>{{ key.slug | upper }}</a></li>
                                        {% endif %}
                                    {% endif %}
                                {% endfor %}
                            </ul>
                        </li>
                        <li class="wt-social-net">
                            <a href="#" title="facebook">
                                <img src="/libs/img/facebook-f-black.svg" alt="facebook" loading="lazy">
                            </a>
                            <a href="#" title="linkedin">
                                <img src="/libs/img/linkedin-black.svg" alt="linkedin" loading="lazy">
                            </a>
                            <a href="#" title="instagram">
                                <img src="/libs/img/instagram-black.svg" alt="instagram" loading="lazy">
                            </a>
                            <p class="wt-share-button" onclick="shareButton(this);" data-href="{{ home_url }}{{ session.language == baselocale ? '' :  session.language~'/' }}{{ path }}{% if not (product_slug is empty) %}{{ '/' ~ p_slug[baselocale] }}{% endif %}{% if c_slug|length > 0 %}{{ '/' ~ c_slug[session.language] }}{% endif %}">
                                <img src="/libs/img/share-button.svg" alt="share" loading="lazy">
                            </p>
                        </li>
                    </ul>
                </nav>
            </div>
        </header>
    </div>