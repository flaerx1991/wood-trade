<section class="wt-services-section">
    <div class="wt-services-container">
       <ul class="wt-services-tabs-buttons">
           <li class="wt-service-tab wt-selected-tab " href="tab-1">
               <div class="wt-tab-img">
                    <picture>
                        <source srcset="{{ image_tab_img_1_webp.path }}" type='image/webp'>
                        <source srcset="{{ image_tab_img_1.path }}" type='image/png'>
                        <img src="{{ image_tab_img_1_webp.path }}" class="img-fluid" alt="{{ image_tab_img_1.alt }}" loading="lazy">
                    </picture>
               </div>
               <span class="wt-service-title">{{ tab_service_name_1 }}</span>
           </li>
           <li class="wt-service-tab" href="tab-2">
               <div class="wt-tab-img">
                    <picture>
                        <source srcset="{{ image_tab_img_2_webp.path }}" type='image/webp'>
                        <source srcset="{{ image_tab_img_2.path }}" type='image/png'>
                        <img src="{{ image_tab_img_2_webp.path }}" class="img-fluid" alt="{{ image_tab_img_2.alt }}" loading="lazy">
                    </picture>
               </div>
               <span class="wt-service-title">{{ tab_service_name_2 }}</span>
           </li>
           <li class="wt-service-tab " href="tab-3">
